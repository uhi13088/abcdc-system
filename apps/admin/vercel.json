{
  "framework": "nextjs",
  "buildCommand": "next build",
  "installCommand": "cd ../.. && pnpm install",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/cron/work-reminder",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/cron/attendance-anomaly-check",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/auto-checkout",
      "schedule": "0 2,23 * * *"
    }
  ]
}
